# Spring Profile (h2 for development, mysql for production)
SPRING_PROFILES_ACTIVE=h2

# MySQL Configuration (only needed when using mysql profile)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=observability
MYSQL_USERNAME=observability
MYSQL_PASSWORD=your_secure_password

# Data Retention (in days)
OBSERVABILITY_RETENTION_METRICS_DAYS=7
OBSERVABILITY_RETENTION_LOGS_DAYS=7
OBSERVABILITY_RETENTION_TRACES_DAYS=7

